<routes>

	<!--车辆类型定义：小型车、大型车-->
	<vType id="vt-car"   vClass="passenger"   color="1,0.2,1" length="5"  maxSpeed="13.89"/><!--50km/h 速度单位：m/s-->
	<vType id="vt-truck" vClass="truck"       color="0,0.2,1" length="10" maxSpeed="11"/>

	
	<!--车辆构成-->
	<vTypeDistribution id="typedist-west"     vTypes="vt-car vt-truck" probabilities="0.88 0.12"/> <!--西进口车辆构成-->
	<vTypeDistribution id="typedist-south"    vTypes="vt-car vt-truck" probabilities="0.83 0.17"/> <!--南进口车辆构成-->
	<vTypeDistribution id="typedist-east"     vTypes="vt-car vt-truck" probabilities="0.88 0.12"/> <!--东进口车辆构成-->
	<vTypeDistribution id="typedist-north"    vTypes="vt-car vt-truck" probabilities="0.83 0.17"/> <!--北进口车辆构成-->
	
	<!--流量的定义-->
	<interval begin="0" end="899">
		<!--西进口-->
		<flow id="west-L0"  from="E0" to="-E1" type="typedist-west"  departLane="best" number="30"/> <!--西左转-->
		<flow id="west-S0"  from="E0" to="-E3" type="typedist-west"  departLane="best" number="450"/> <!--西直行-->
		<flow id="west-R0"  from="E0" to="-E2" type="typedist-west"  departLane="best" number="30"/> <!--西右转-->
		<!--南进口-->
		<flow id="south-L0" from="E2" to="-E0" type="typedist-south" departLane="best" number="30"/> <!--南左转-->
		<flow id="south-S0" from="E2" to="-E1"  type="typedist-south" departLane="best" number="300"/> <!--南直行-->
		<flow id="south-R0" from="E2" to="-E3"  type="typedist-south" departLane="best" number="30"/> <!--南右转-->
		<!--东进口-->
		<flow id="east-L0"  from="E3" to="-E2"  type="typedist-east"  departLane="best" number="30"/> <!--东左转-->
		<flow id="east-S0"  from="E3" to="-E0" type="typedist-east"  departLane="best" number="450"/> <!--东直行-->
		<flow id="east-R0"  from="E3" to="-E1"  type="typedist-east"  departLane="best" number="30"/> <!--东右转-->
		<!--北进口-->
		<flow id="north-L0" from="E1" to="-E3"  type="typedist-north" departLane="best" number="30"/> <!--北左转-->
		<flow id="north-S0" from="E1" to="-E2"  type="typedist-north" departLane="best" number="300"/> <!--北直行-->
		<flow id="north-R0" from="E1" to="-E0"  type="typedist-north" departLane="best" number="30"/> <!--北右转-->
	</interval>

	<interval begin="900" end="1799">
		<!--西进口-->
		<flow id="west-L1"  from="E0" to="-E1" type="typedist-west"  departLane="best" number="30"/> <!--西左转-->
		<flow id="west-S1"  from="E0" to="-E3" type="typedist-west"  departLane="best" number="450"/> <!--西直行-->
		<flow id="west-R1"  from="E0" to="-E2" type="typedist-west"  departLane="best" number="30"/> <!--西右转-->
		<!--南进口-->
		<flow id="south-L1" from="E2" to="-E0" type="typedist-south" departLane="best" number="30"/> <!--南左转-->
		<flow id="south-S1" from="E2" to="-E1"  type="typedist-south" departLane="best" number="300"/> <!--南直行-->
		<flow id="south-R1" from="E2" to="-E3"  type="typedist-south" departLane="best" number="30"/> <!--南右转-->
		<!--东进口-->
		<flow id="east-L1"  from="E3" to="-E2"  type="typedist-east"  departLane="best" number="30"/> <!--东左转-->
		<flow id="east-S1"  from="E3" to="-E0" type="typedist-east"  departLane="best" number="450"/> <!--东直行-->
		<flow id="east-R1"  from="E3" to="-E1"  type="typedist-east"  departLane="best" number="30"/> <!--东右转-->
		<!--北进口-->
		<flow id="north-L1" from="E1" to="-E3"  type="typedist-north" departLane="best" number="30"/> <!--北左转-->
		<flow id="north-S1" from="E1" to="-E2"  type="typedist-north" departLane="best" number="300"/> <!--北直行-->
		<flow id="north-R1" from="E1" to="-E0"  type="typedist-north" departLane="best" number="30"/> <!--北右转-->
	</interval>

	<interval begin="1800" end="2699">
		<!--西进口-->
		<flow id="west-L2"  from="E0" to="-E1" type="typedist-west"  departLane="best" number="30"/> <!--西左转-->
		<flow id="west-S2"  from="E0" to="-E3" type="typedist-west"  departLane="best" number="450"/> <!--西直行-->
		<flow id="west-R2"  from="E0" to="-E2" type="typedist-west"  departLane="best" number="30"/> <!--西右转-->
		<!--南进口-->
		<flow id="south-L2" from="E2" to="-E0" type="typedist-south" departLane="best" number="30"/> <!--南左转-->
		<flow id="south-S2" from="E2" to="-E1"  type="typedist-south" departLane="best" number="300"/> <!--南直行-->
		<flow id="south-R2" from="E2" to="-E3"  type="typedist-south" departLane="best" number="30"/> <!--南右转-->
		<!--东进口-->
		<flow id="east-L2"  from="E3" to="-E2"  type="typedist-east"  departLane="best" number="30"/> <!--东左转-->
		<flow id="east-S2"  from="E3" to="-E0" type="typedist-east"  departLane="best" number="450"/> <!--东直行-->
		<flow id="east-R2"  from="E3" to="-E1"  type="typedist-east"  departLane="best" number="30"/> <!--东右转-->
		<!--北进口-->
		<flow id="north-L2" from="E1" to="-E3"  type="typedist-north" departLane="best" number="30"/> <!--北左转-->
		<flow id="north-S2" from="E1" to="-E2"  type="typedist-north" departLane="best" number="300"/> <!--北直行-->
		<flow id="north-R2" from="E1" to="-E0"  type="typedist-north" departLane="best" number="30"/> <!--北右转-->
	</interval>

	<interval begin="2700" end="3599">
		<!--西进口-->
		<flow id="west-L3"  from="E0" to="-E1" type="typedist-west"  departLane="best" number="30"/> <!--西左转-->
		<flow id="west-S3"  from="E0" to="-E3" type="typedist-west"  departLane="best" number="450"/> <!--西直行-->
		<flow id="west-R3"  from="E0" to="-E2" type="typedist-west"  departLane="best" number="30"/> <!--西右转-->
		<!--南进口-->
		<flow id="south-L3" from="E2" to="-E0" type="typedist-south" departLane="best" number="30"/> <!--南左转-->
		<flow id="south-S3" from="E2" to="-E1"  type="typedist-south" departLane="best" number="300"/> <!--南直行-->
		<flow id="south-R3" from="E2" to="-E3"  type="typedist-south" departLane="best" number="30"/> <!--南右转-->
		<!--东进口-->
		<flow id="east-L3"  from="E3" to="-E2"  type="typedist-east"  departLane="best" number="30"/> <!--东左转-->
		<flow id="east-S3"  from="E3" to="-E0" type="typedist-east"  departLane="best" number="450"/> <!--东直行-->
		<flow id="east-R3"  from="E3" to="-E1"  type="typedist-east"  departLane="best" number="30"/> <!--东右转-->
		<!--北进口-->
		<flow id="north-L3" from="E1" to="-E3"  type="typedist-north" departLane="best" number="30"/> <!--北左转-->
		<flow id="north-S3" from="E1" to="-E2"  type="typedist-north" departLane="best" number="300"/> <!--北直行-->
		<flow id="north-R3" from="E1" to="-E0"  type="typedist-north" departLane="best" number="30"/> <!--北右转-->
	</interval>

</routes>